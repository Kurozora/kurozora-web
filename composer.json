{
    "name": "musa11971/kurozora-web",
    "description": "Kurozora web application.",
    "keywords": [],
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-simplexml": "*",
        "anaseqal/nova-sidebar-icons": "^0.0.2",
        "bensampo/laravel-enum": "^1.29",
        "chaseconey/nova-external-image": "^0.0.3",
        "cybercog/laravel-love": "^5.2",
        "ebess/advanced-nova-media-library": "^2.9",
        "facade/ignition": "^1.16",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "laraning/nova-time-field": "^0.2.0",
        "laravel-notification-channels/apn": "^1.3",
        "laravel/framework": "^6.0",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "musa11971/laravel-sort-request": "^1.0",
        "musa11971/laravel-tvdb": "^1.0",
        "nicolaslopezj/searchable": "1.*",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^3.6",
        "timothyasp/nova-color-field": "^1.0",
        "vyuldashev/nova-permission": "^2.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/tinkerwell-helper": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^8.0",
        "spatie/phpunit-snapshot-assertions": "^3.1"
    },
    "autoload": {
        "files": [
            "app/Helpers/CustomHelpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Helpers"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
