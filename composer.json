{
    "name": "musa11971/kurozora-web",
    "description": "Kurozora web application.",
    "keywords": [],
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-simplexml": "*",
        "anaseqal/nova-sidebar-icons": "^0.0",
        "bensampo/laravel-enum": "^2.0",
        "chaseconey/nova-external-image": "^0.2",
        "codercat/jwk-to-pem": "^0.0.3",
        "cybercog/laravel-love": "^8.0",
        "ebess/advanced-nova-media-library": "^3.0",
        "fideloper/proxy": "^4.0",
        "itsgoingd/clockwork": "^4.1",
        "laraning/nova-time-field": "^1.0",
        "laravel-notification-channels/apn": "^2.0",
        "laravel/framework": "^7.0",
        "laravel/nova": "*",
        "laravel/tinker": "^2.0",
        "musa11971/laravel-sort-request": "^1.0",
        "musa11971/laravel-tvdb": "^1.0",
        "musa11971/php-jwt-decoder": "^1.0",
        "nicolaslopezj/searchable": "1.*",
        "nikkanetiya/laravel-color-palette": "dev-larave-6-7",
        "spatie/icalendar-generator": "^1.0",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-permission": "^3.6",
        "spatie/laravel-sluggable": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.11",
        "timothyasp/nova-color-field": "^1.0",
        "vyuldashev/nova-permission": "^2.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "git",
            "url": "https://github.com/pmayet/laravel-color-palette/"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/tinkerwell-helper": "^1.1",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "files": [
            "app/Helpers/CustomHelpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Helpers"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan nova:publish",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
