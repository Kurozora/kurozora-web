{
    "name": "kurozora/kurozora-web",
    "type": "project",
    "description": "Kurozora web application.",
    "keywords": ["kurozora", "anime", "manga", "game", "character", "actor", "people", "studio", "hentai", "song", "music", "track", "watch"],
    "funding": [
        {
            "type": "patreon",
            "url": "https://patreon.com/kurozora"
        },
        {
            "type": "paypal",
            "url": "https://paypal.com/paypalme/kiritokatklian"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "astrotomic/laravel-translatable": "^11.10",
        "aws/aws-sdk-php": "^3.198",
        "bacon/bacon-qr-code": "^2.0",
        "bensampo/laravel-enum": "^5.1",
        "blade-ui-kit/blade-icons": "^1.0",
        "codercat/jwk-to-pem": "^1.0",
        "cybercog/laravel-love": "^8.0",
        "ebess/advanced-nova-media-library": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "halaxa/json-machine": "^1.0",
        "hisorange/browser-detect": "^4.4",
        "http-interop/http-factory-guzzle": "^1.2",
        "imdhemy/laravel-purchases": "^1.0",
        "intervention/image": "^2.7",
        "jaybizzle/crawler-detect": "^1.2",
        "kabbouchi/nova-logs-tool": "^2.0",
        "kiritokatklian/laravel-color-palette": "^2.0",
        "kiritokatklian/nova-astrotranslatable": "^2.0",
        "kiritokatklian/nova-permission": "^4.0",
        "laravel-notification-channels/apn": "^v4.0",
        "laravel/framework": "^9.2",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.14",
        "laravel/scout": "^9.4",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "lukeraymonddowning/honey": "^0.4",
        "marvinlabs/laravel-discord-logger": "^1.2",
        "meilisearch/meilisearch-php": "^0.23.2",
        "musa11971/laravel-sort-request": "dev-php-7.4-and-8.0-support",
        "musa11971/php-jwt-decoder": "dev-php-7.4-and-8.0-support",
        "nicolaslopezj/searchable": "1.*",
        "outl1ne/nova-color-field": "^2.0",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^7.0",
        "roach-php/laravel": "^1.0",
        "socialiteproviders/apple": "^5.0",
        "spatie/browsershot": "^3.57",
        "spatie/icalendar-generator": "^2.3",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^5.0",
        "spatie/laravel-sitemap": "^6.0",
        "spatie/laravel-sluggable": "^3.0",
        "timothyasp/nova-color-field": "^2.0",
        "titasgailius/search-relations": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "brianium/paratest": "^6.4",
        "fakerphp/faker": "^1.19",
        "friendsofphp/php-cs-fixer": "^3.9",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "git",
            "url": "https://github.com/kiritokatklian/php-jwt-decoder"
        },
        {
            "type": "git",
            "url": "https://github.com/kiritokatklian/laravel-sort-request"
        }
    ],
    "autoload": {
        "files": [
            "app/Helpers/CustomHelpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Helpers\\": "app/Helpers/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan nova:publish",
            "@php artisan vendor:publish --force --tag=livewire:assets",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N",
            "@php artisan ide-helper:meta"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "format": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "google/apiclient-services": [
            "AndroidPublisher"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
