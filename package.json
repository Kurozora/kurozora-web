        "popper.js": "^1.16.1",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@jaames/iro": "^4.5.3",
        "tailwindcss": "^1.5.2",
