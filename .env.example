# Application general information
APP_NAME=Kurozora
APP_ENV=local
APP_DEBUG=true
APP_KEY=
DOMAIN_NAME=kurozora-web.test
STATUS_URL=https://status.{$DOMAIN_NAME}
APP_BUNDLE_ID=app.kurozora.tracker

# Social
TWITTER_HANDLE=KurozoraApp
INSTAGRAM_HANDLE=kurozora_app
REDDIT_HANDLE=kurozora
DISCORD_INVITE=f3QFzGqsah

# Services
PATREON_HANDLE=kurozora
PAYPAL_HANDLE=Kiritokatklian

# Application logging
LOG_CHANNEL=stack
LOG_LEVEL=debug
LOG_DISCORD_WEBHOOK_URL=

# Database connection
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=homestead
DB_USERNAME=homestead
DB_PASSWORD=secret

# Kurozora Dashboard connection
K_DB_CONNECTION=pgsql
K_DB_HOST=127.0.0.1
K_DB_PORT=5432
K_DB_DATABASE=homestead
K_DB_USERNAME=homestead
K_DB_PASSWORD=secret

# Drivers
BROADCAST_DRIVER=
CACHE_DRIVER=database
FILESYSTEM_DISK=project
MEDIA_DISK=public
NOVA_STORAGE_DISK=public
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true
SESSION_COOKIE_PREFIX=__Secure-
SCOUT_DRIVER=meilisearch
QUEUE_DRIVER=database
QUEUE_CONNECTION=database

# APN service information
# See https://github.com/laravel-notification-channels/apn
APN_KEY_ID=
APN_TEAM_ID=
APN_BUNDLE_ID=app.kurozora.tracker
APN_PRIVATE_KEY_CONTENT="-----BEGIN PRIVATE KEY-----\nxxx\n-----END PRIVATE KEY-----"

# Sign in with Apple
# Should update every 6 months (max expiration date)
APPLE_CLIENT_ID=app.kurozora.web.tracker
MIX_APPLE_CLIENT_SECRET=
APPLE_REDIRECT_URI=/siwa/callback

# AWS S3
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=eu-central-1
AWS_BUCKET=
AWS_URL=https://cdn.kurozora.app/

# AWS SES
SES_KEY=
SES_SECRET=
SES_REGION=eu-central-1

# Laravel Log Viewer
LOG_VIEWER_ENABLE_ROUTES=false
NOVA_LOGS_PER_PAGE=25
NOVA_LOGS_REGEX_FOR_FILES="/^laravel|^worker/"

# Mail configuration
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# MeiliSearch
MEILISEARCH_HOST=http://127.0.0.1:7700
MEILISEARCH_KEY=

# Memcached
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PORT=11211
MEMCACHED_USERNAME=
MEMCACHED_PASSWORD=


# reCAPTCHA
RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET_KEY=

# Redis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Scout
SCOUT_QUEUE=true
SCOUT_IDENTIFY=false

# StoreKit Configuration
SK_APP_PASSWORD=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
